{
  "patterns": {
    "Generic API Key": "(?i)(api_key|secret_key|token|access_token|client_secret|auth_token)=['\"]?[a-zA-Z0-9_-]{16,64}['\"]?",
    "AWS Access Key ID": "AKIA[0-9A-Z]{16}",
    "AWS Secret Access Key": "([A-Za-z0-9+/]{40})",
    "Azure Subscription Key": "[a-f0-9]{32}",
    "Google Cloud API Key": "AIza[0-9A-Za-z\\-_]{35}",
    "Stripe Secret Key": "(sk_live|sk_test)_[0-9a-zA-Z]{24,}",
    "GitHub Personal Access Token (classic)": "ghp_[0-9a-zA-Z]{36}",
    "GitHub Personal Access Token (fine-grained)": "gh[oups]_[0-9a-zA-Z]{36}",
    "Slack Bot Token": "xoxb-[0-9]{11}-[0-9]{11}-[0-9a-zA-Z]{24}",
    "Firebase Web API Key": "AIza[0-9A-Za-z-_]{35}",
    "Generic Password": "(?i)(password|pwd|pass|secret)(?=.*[a-zA-Z0-9!@#$%^&*()_+]{8,})",
    "JWT Token": "eyJ[A-Za-z0-9-_=]+\\.[A-Za-z0-9-_=]+\\.[A-Za-z0-9-_.+/=]+",
    "SSH Private Key": "-----BEGIN (RSA|DSA|EC|OPENSSH) PRIVATE KEY-----",
    "PGP Private Key Block": "-----BEGIN PGP PRIVATE KEY BLOCK-----",
    "SQL Connection String (basic)": "(?i)(jdbc:postgresql|mysql|mssql|sqlite)://([a-zA-Z0-9_.-]+:[a-zA-Z0-9_.-]+@)?[a-zA-Z0-9_.-]+(:[0-9]+)?/[a-zA-Z0-9_.-]+",
    "Bearer Token": "(?i)Bearer\\s[A-Za-z0-9-_=]+\\.[A-Za-z0-9-_=]+\\.[A-Za-z0-9-_.+/=]+"
  }
}